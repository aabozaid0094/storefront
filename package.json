    "jasmine": "^4.2.1",
    "jasmine-spec-reporter": "^7.0.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  }
